//using the groovy lang example
class Person {
    String name
    Integer age
    Person (name, age){
      this.name = name
      this.age = age
      echo "newperson"
    }
    def increaseAge(Integer years) {
        this.age += years
    }
}
def person1 = new Person('john', 35)
